    "title": "Litore - The beach cleaning crab",
    "icon": "",
    "thumbnail": "https://lh3.googleusercontent.com/drive-viewer/AFGJ81oEVjOn5eVhw0QB7JPTsSwPpsDtXCYR9OjfhGXWs6_dhAY0Og4t7_a55iJpZZKc1H4jjE7FIBMoqay1wsFe3hpeu2V0-w=s1600",
    "status": "Completed",
    "timeline": [[2021, 9], [2022,2]],
    "labels": [
      "Drawing",
      "Electronics",
      "CAD",
      "Render",
      "Prototype",
